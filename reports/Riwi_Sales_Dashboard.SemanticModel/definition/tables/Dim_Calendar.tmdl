	partition Dim_Calendar = calculated
		mode: import
		source =
				
				VAR MinDate = MIN(Fact_Sales[Fecha])
				VAR MaxDate = MAX(Fact_Sales[Fecha])
				RETURN
				ADDCOLUMNS (
				    CALENDAR (MinDate, MaxDate),
				    "Year", YEAR([Date]),
				    "Month No", MONTH([Date]),
				    "Month Name", FORMAT([Date], "MMMM"),
				    "Year-Month", FORMAT([Date], "YYYY-MM"),
				    "Week Start", [Date] - WEEKDAY([Date], 2) + 1,  -- Since monday
				    "Week Num", WEEKNUM([Date], 2)
				)

	annotation PBI_Id = 672f20b0214a49c7abd8d800c2c44e81

